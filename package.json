  "license": "MIT",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && tsc && npm run concat && npm run minify",
    "concat": "cat node_modules/tracekit/tracekit.js dist/index.js > dist/ionic-error-tracking.js",
    "minify": "uglifyjs dist/ionic-error-tracking.js -o dist/ionic-error-tracking.min.js --compress --mangle && rm dist/index.js dist/ionic-error-tracking.js",
    "clean": "rimraf ./dist"
  },
  "main": "dist/ionic-error-tracking.min.js",
  "devDependencies": {
    "rimraf": "^2.6.1",
    "tracekit": "^0.4.4",
    "typescript": "~2.2.1",
    "uglify-js": "2.8.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/ionic-error-tracking.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/ionic-error-tracking/issues"
  }
}
